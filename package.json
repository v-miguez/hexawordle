{
  "name": "hexawordle",
  "version": "1.0.0",
  "scripts": {
    "start": "concurrently \"npm run start-angular\" \"npm run start-express\"",
    "start-angular": "cd hexawordle && npm start",
    "start-express": "cd api && npm start",
    "install-deps": "concurrently \"cd hexawordle && npm install\" \"cd api && npm install\""
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
